{
  "name": "muxe-de-blog",
  "private": true,
  "workspaces": ["server", "client"],
  "version": "1.0.0",
  "description": "my blog",
  "main": "./server/js/server.js",
  "scripts": {
    "subinstall": "(cd server && npm install) && (cd client && npm install)",
    "test": "echo 'no testing right now'",
    "serve-ui": "browser-sync start -c bs-config.js",
    "deploy": "bash ./scripts/deploy.sh",
    "build": "npm run build-api",
    "build-api": "cd server && npm run build",
    "serve-api": "cd server && npm run serve",
    "start": "cd server && npm start",
    "pull-posts": "bash ./scripts/pull-posts.sh",
    "push-posts": "bash ./scripts/push-posts.sh"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:mbarz/blog.git"
  },
  "author": "Max-Richard Barz",
  "license": "ISC",
  "devDependencies": {
    "browser-sync": "^2.23.6",
    "http-proxy": "^1.16.2"
  },
  "dependencies": {}
}
