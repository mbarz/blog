{
  "name": "muxe-de-blog-server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "del js && tsc -p .",
    "build:w": "tsc -w -p .",
    "serve": "nodemon --watch 'ts/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' ts/server.ts",
    "serve:js": "nodemon --watch 'ts/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'node' ts/server.tj",
    "start": "node js/server.js"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/express": "^4.11.1",
    "@types/node": "^9.6.0",
    "@types/passport": "^0.4.4",
    "del-cli": "^1.1.0",
    "nodemon": "^1.17.2",
    "ts-node": "^5.0.1",
    "typescript": "^2.8.1"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "chalk": "^2.3.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.3",
    "express-history-api-fallback": "^2.2.1",
    "express-session": "^1.15.6",
    "left-pad": "^1.2.0",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "session-file-store": "^1.2.0"
  }
}
